<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Testing">
	<listeners>
		<listener class-name="com.report.ExtentReporter" />
		<listener class-name="com.report.ScreenshotListener" />
	</listeners>

	  <test name="FunctionalDashboardPage" preserve-order="true">
		<groups>
			<run>
				<include name="preProdSanity" />
				<include name="prodSanity" />
				<include name="regression" />
			</run> 
		</groups>
		<classes>
			<class name="com.dashboard.FunctionalDashboardPage"></class>
		</classes>
	</test>   

</suite>